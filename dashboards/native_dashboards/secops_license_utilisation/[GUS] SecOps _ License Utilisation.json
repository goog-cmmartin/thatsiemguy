            "dashboardQueries": [
                {
                    "name": "0a5e2f46-2352-44e8-976d-7a1e72f6105d",
                    "query": "ingestion.component = \"Ingestion API\"\n\noutcome:\n    $Count = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\n",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "7"
                        }
                    },
                    "etag": "38ad1d87885e362af84ddfc7c1a7468bd364b6fbfbe6a7163dfb53a3a503ffc8"
                },
                {
                    "name": "98b9200d-88c6-452c-810a-8e9517b6a12c",
                    "query": "ingestion.component = \"Ingestion API\"\n\noutcome:\n    $Count = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\n",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "YEAR",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "54111c0eca41fbe59a10cc44279ece0ff20abec366a1ba2d4ed16f780b7e45ab"
                },
                {
                    "name": "2b51033e-8731-43df-b915-b231a388bc53",
                    "query": "ingestion.component = \"Ingestion API\"\n$date = timestamp.get_date(ingestion.end_time) \nmatch:\n    $date\noutcome:\n    $GB = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\norder:\n    $date asc",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "YEAR",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "dba7aee014bedf9fe338b2dc771d45288da93bf27ef09d109da85902a216404c"
                },
                {
                    "name": "d50107b4-af62-4dc6-9878-186fbd389b77",
                    "query": "ingestion.component = \"Ingestion API\"\n\noutcome:\n    $Count = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\n",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "30"
                        }
                    },
                    "etag": "7ed96355275fb26d7d1549ef688680fe4fabafe3b65dc87c2edb42a9fa30fc68"
                },
                {
                    "name": "3a2b814a-bf01-4a48-ac8d-e68e2ceb7f54",
                    "query": "ingestion.component = \"Ingestion API\"\n\noutcome:\n    $Count = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\n",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "3181d090cf274626c098a8312a955816f7b9b1af6718ea49a9ecb7443f9ad912"
                },
                {
                    "name": "017aaf8b-9900-4eb0-a4c8-dba6e5976d0c",
                    "query": "ingestion.component = \"Ingestion API\"\ningestion.log_type = $logType\nmatch: \n    $logType\noutcome:\n    $GB = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\n    $logCount = sum(ingestion.log_count)    \norder:\n    $GB desc",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "YEAR",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "494e737845e5ddc1c74a94060f9abd6344406590557e38ad06db4024a0d40a32"
                },
                {
                    "name": "2cbf2c8d-7ca9-487f-934f-c0e441be9992",
                    "query": "ingestion.component = \"Ingestion API\"\noutcome:\n    $partitionCount = math.round( ( max(ingestion.end_time) - min(ingestion.end_time) ) / 86400, 2)",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "YEAR",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "b2115e95ca7ce51a9306ec21ac08259fd26f176302081af45a6fbcb39e563144"
                },
                {
                    "name": "332f526c-10df-4473-967d-462fa5e2d151",
                    "query": "ingestion.component = \"Ingestion API\"\noutcome:\n    $totalEvents = sum(ingestion.log_count) \n    $time = ( max(ingestion.end_time) - min(ingestion.end_time) )\n    $eps = math.round($totalEvents / $time, 2)",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "YEAR",
                            "startTimeVal": "1"
                        }
                    },
