            "dashboardQueries": [
                {
                    "name": "d4acf9f6-8073-4439-a06b-eaee3cdedae5",
                    "query": "ingestion.component = \"Ingestion API\"\n\noutcome:\n    $Count = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\n",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "c22f2eca0dc4bd7850a869f00f3f4799711903d2bec1b114c0982f41d102170e"
                },
                {
                    "name": "3eb82c28-47f2-4d3d-8740-a9e20e0dea6f",
                    "query": "ingestion.component = \"Ingestion API\"\ningestion.log_type = $logType\nmatch: \n    $logType\noutcome:\n    $GB = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\n    $logCount = sum(ingestion.log_count)    \norder:\n    $GB desc",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "YEAR",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "492049a3e04630d201877e22965559ef3eddad36d3eaa6f2fa5656a1b08bafa0"
                },
                {
                    "name": "6d07f56c-7a17-480d-af64-cbfc80b3b531",
                    "query": "ingestion.component = \"Ingestion API\"\noutcome:\n    $partitionCount = math.round( ( max(ingestion.end_time) - min(ingestion.end_time) ) / 86400, 2)",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "YEAR",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "89fdada84712dc4254800ba18d0839d414134c919ee54c37c41b307c156ad4ce"
                },
                {
                    "name": "934a8bdf-2b7a-4214-a7a4-3a63f7b8a333",
                    "query": "ingestion.component = \"Ingestion API\"\noutcome:\n    $totalEvents = sum(ingestion.log_count) \n    $time = ( max(ingestion.end_time) - min(ingestion.end_time) )\n    $eps = math.round($totalEvents / $time, 2)",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "YEAR",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "ff63cc5af78a24c72f6fa49b24557cd3952964a8820c41436ceecf453183d9bc"
                },
