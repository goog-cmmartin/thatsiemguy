            "dashboardQueries": [
                {
                    "name": "4e2c4fad-b1e7-407b-8e34-552063405808",
                    "query": "ingestion.component = \"Ingestion API\"\n\noutcome:\n    $Count = math.round(sum(ingestion.log_volume) / 1000 / 1000 / 1000, 4)\n",
                    "input": {
                        "relativeTime": {
