            "dashboardQueries": [
                {
                    "name": "c7b3926d-4d8e-49c1-81b3-88ca6ed27576",
                    "query": "metadata.log_type = \"SDL_GTI_DTM\"\ntimestamp.get_date(metadata.event_timestamp.seconds) = $date\nmatch:\n    $date\noutcome:\n    $high_confidence = sum(if(security_result.confidence_score * 100>=80,1,0))\n    $medium_confidence = sum(if(security_result.confidence_score * 100 >=40 and security_result.confidence_score<80,1,0))  \n    $low_confidence = sum(if(security_result.confidence_score * 100 >=0 and security_result.confidence_score<40,1,0))        \norder:\n    $date desc",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "7"
                        }
                    },
                    "etag": "33e1511f260863179d4d9c6262a798f323ca980fea1c78db60d1bf9981d8338a"
                },
                {
                    "name": "7e876296-15f9-41b6-9b13-202c088cc995",
                    "query": "metadata.log_type = \"SDL_GTI_DTM\"\nmetadata.product_event_type = $alertType\nsecurity_result.confidence_score = $confidenceScore\nsecurity_result.rule_name = $monitorId\n//security_result.rule_id = $monitorId\n//extracted.fields[\"monitor_id\"] = $monitorId\ntarget.hostname = $domain\nmetadata.description = $summary\ntimestamp.get_timestamp(metadata.event_timestamp.seconds) = $date\nmatch:\n    $monitorId, $alertType\noutcome:\n    $total = count(metadata.id)\n    $timeDifference = max(metadata.event_timestamp.seconds) - min(metadata.event_timestamp.seconds) / 86400\n    $average_per_day = $total / $timeDifference\norder:\n    $total desc",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "71b6346aefaf9d59f0c3531b65d79c415178cd8aeb973a6148236d14faf615bb"
                },
                {
                    "name": "37a90c44-097a-47fd-8e42-cc542bbda7f9",
                    "query": "metadata.log_type = \"SDL_GTI_DTM\"\nmetadata.product_event_type = $productEventType\n$date = timestamp.get_timestamp(metadata.event_timestamp.seconds)\nmatch:\n    $date, $productEventType\noutcome:\n    $total = count(metadata.id)\norder:\n    $total desc",
                    "input": {
